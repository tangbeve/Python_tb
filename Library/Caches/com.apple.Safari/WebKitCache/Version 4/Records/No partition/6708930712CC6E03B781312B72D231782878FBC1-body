jQuery(document).ready(function($){
  hr_metrics.track("PageLoad", window.location.pathname, {});
  $(document).on('click', 'a', null, function(e) {
      src = $(e.currentTarget);
      action = 'Click'; 
      if (src.attr('data-analytics')) {
          data = src.attr('data-analytics');
      } else {
          tagname = src.prop("tagName").toUpperCase();
          if (tagname == "A") {
            data = $.trim(src.text());
          }
          else {
            return;
          }
      }

      hr_metrics.track(action, data, (function() {
        var params={};
        for (var _i=1; _i<=11; ++_i){
          var _attr = 'data-attr'+_i;
          if (src.attr(_attr)){
            params['attribute'+_i] = src.attr(_attr);
        }}
        return params;
      })());
      // google analytics
      //_gaq.push(['_trackEvent', 'Events' , action, data])
  });
});
